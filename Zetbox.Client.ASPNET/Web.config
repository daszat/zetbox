      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Zetbox.Client" publicKeyToken="ccdf16e4dd7b6d78" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.0.0" newVersion="2.2.0.0" />
      </dependentAssembly>
