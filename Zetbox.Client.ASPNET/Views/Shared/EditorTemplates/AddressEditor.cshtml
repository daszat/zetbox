@model Zetbox.Client.Presentables.ValueViewModels.CompoundObjectPropertyViewModel

@using Zetbox.Client.ASPNET

<div class="form-group">
    <label class="col-md-3 control-label">Adresse</label>
    <div class="col-md-6">
        @Html.EditorFor(model => model.Value.PropertyModelsByName["Line1"])
    </div>
</div>
<div class="form-group">
    <div class="col-md-offset-3 col-md-6">
        @Html.EditorFor(model => model.Value.PropertyModelsByName["Line2"])
    </div>
</div>
<div class="form-group">
    <label class="col-md-3 control-label">PLZ/Ort</label>
    <div class="col-md-2">
        @Html.EditorFor(model => model.Value.PropertyModelsByName["ZIPCode"])
    </div>
    <div class="col-md-4">
        @Html.EditorFor(model => model.Value.PropertyModelsByName["City"])
    </div>
</div>
