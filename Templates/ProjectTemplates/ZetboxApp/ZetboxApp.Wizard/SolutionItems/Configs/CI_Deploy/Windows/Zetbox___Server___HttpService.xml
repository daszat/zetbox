<?xml version="1.0" encoding="Windows-1252"?>
<ZetboxConfig xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://dasz.at/Zetbox/">
  <ConfigName>$safesolutionname$ HttpService Configuration</ConfigName>
  <Server StartServer="true">
    <ConnectionStrings />
    <DocumentStore>..\DocumentStore\Server\</DocumentStore>
    <Modules>
      <Module>Zetbox.Server.ServerModule, Zetbox.Server</Module>
      <Module>Zetbox.DalProvider.Ef.EfProvider, Zetbox.DalProvider.Ef</Module>
      <Module NotOnFallback="true">$safesolutionname$.Server.ServerModule, $safesolutionname$.Server</Module>
    </Modules>
    <ClientFilesLocations>
      <Location Name="Exe" Start="true">..\$safesolutionname$.WPF.exe</Location>
      <Location Name="Configs">..\Configs\$safesolutionname$.WPF.xml</Location>
      <Location Name="Common">..\Common</Location>
      <Location Name="Client">..\Client</Location>
    </ClientFilesLocations>
  </Server>
  <AssemblySearchPaths EnableShadowCopy="true">
    <string>..\Common\Core</string>
    <string>..\Server\Core</string>
    <string>..\Server\EF</string>
    <string>..\Common\App.Zetbox</string>
    <string>..\Server\App.Zetbox</string>
    <string>..\Server\App.$safesolutionname$</string>
    <string>..\Common\App.$safesolutionname$</string>
  </AssemblySearchPaths>
</ZetboxConfig>